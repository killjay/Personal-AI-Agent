# Environment Variables for GPT-OSS Voice AI Agent
# Copy this file to .env and update with your actual values

# OpenAI API Key (if you want to use OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face Token (for downloading models)
HUGGINGFACE_TOKEN=your_huggingface_token_here

# Google OAuth2 Credentials (for Gmail, Calendar, etc.)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Server Configuration
SERVER_HOST=0.0.0.0
TTS_PORT=5002
GPTOSS_PORT=5003
WHISPER_PORT=5004
MAIN_SERVER_PORT=5005
MCP_PORT=8080

# Model Configuration
MODEL_NAME=distilgpt2
MODEL_CACHE_DIR=./models

# Debug Settings
DEBUG=false
LOG_LEVEL=INFO
